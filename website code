<script type="text/javascript">
    require([
        'jquery',
        'owl.carousel/owl.carousel.min'            
        ], function ($) {
         $(window).load(function () {
            function alignHeight(selector) {
                jQuery(selector).css('height', '');
                var minHeight = 0;
                jQuery(selector).each(function () {
                    if (minHeight < jQuery(this).outerHeight()) {
                        minHeight = jQuery(this).outerHeight();
                    }
                });
                if (minHeight > 0) {
                    jQuery(selector).css('height', minHeight);

                }
            }

            /*ALIGN HEIGHT PRODUCT */
            setInterval(function() {
                alignHeight('.catalog-product-view .product-info-main .block.related .block-content .products-grid .item .product-item-info .product-item-details .product-item-name');
            }, 1500);
        });

     });


 </script></div><div class="product media"><a id="gallery-prev-area" tabindex="-1"></a>
